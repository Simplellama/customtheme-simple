<style>


    .tee-searching-animate:not(.hide) {
        display: block;
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(255, 255, 255, 0.7);
        z-index: 99;
        transition: all 0.3s;
    }

    .tee-searching-animate-inner {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        text-align: center;
    }

    .tee-searching-animate .bundle-tee-icon svg {
        width: 80px;
        height: 80px;
    }

    .tee-searching-animate .bundle-search-icon {
        width: 50px;
        height: 50px;
        animation: orbit 2s infinite linear;
        position: absolute;
        top: 50px;
        left: 50%;
    }

    .tee-searching-animate .bundle-search-icon svg {
        width: 70px;
        height: 70px;
        animation: orbit-reverse 2s infinite linear;
    }

    @-webkit-keyframes orbit {
        from {
            -webkit-transform: rotate(0deg) translate(-50%, -50%)
        }

        to {
            -webkit-transform: rotate(360deg) translate(-50%, -50%)
        }
    }

    @-webkit-keyframes orbit-reverse {
        from {
            -webkit-transform: rotate(360deg)
        }

        to {
            -webkit-transform: rotate(0deg)
        }
    }

    .icon__fallback-text {
        clip: rect(0, 0, 0, 0);
        overflow: hidden;
        position: absolute;
        height: 1px;
        width: 1px;
    }

    .icon-search,
    .icon-tee {
        display: inline-block;
        width: 20px;
        height: 20px;
        vertical-align: middle;
        fill: currentColor;
    }

    .icon-search {
        fill: none !important;
        stroke: black !important;

    }
</style>
